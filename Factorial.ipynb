    "#Factorial of the number\n",
    "num=int(input('Enter the number'))\n",
    "fact=1\n",
    "if num>0:\n",
    "    for i in range(1,num+1):\n",
    "        fact=fact*i\n",
    "print(\"The factorial num is :\",fact)  "
